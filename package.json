{
  "name": "guildlens",
  "version": "1.0.0",
  "description": "GuildLens - Community Strategy Discord Bot for analyzing server activity and generating actionable insights",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "npx kill-port 3000 3001 && node index.js",
    "deploy": "node scripts/deployCommands.js",
    "deploy-commands": "node scripts/deployCommands.js",
    "setup": "npm run deploy && npm run start",
    "restart": "npx kill-port 3000 3001 && npm run start",
    "full-setup": "npx kill-port 3000 3001 && npm run deploy && npm run start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "discord",
    "bot",
    "analytics",
    "community",
    "strategy",
    "insights",
    "postgres",
    "supabase"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4"
  }
}